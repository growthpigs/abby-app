{
  "expo": {
    "name": "Abby",
    "slug": "abby-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.properdress.abby",
      "deploymentTarget": "16.0",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Abby needs your microphone to have a conversation with you.",
        "UIBackgroundModes": [
          "audio"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "bitcode": false
    },
    "plugins": [
      "@livekit/react-native-expo-plugin",
      "@config-plugins/react-native-webrtc",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "c4046513-5142-44f2-a28f-643327f71a91"
      }
    }
  }
}
